                            <fo:block role="Sect" id="unique_4" chfox:nd-id="c_performing_arts">
                                <fo:block-container font-size="1.7em" line-height="1.7em" space-after="0.5em" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid" id="ariaid-title4"><fo:block end-indent="0" start-indent="0" role="H2"><fo:inline>Performing arts</fo:inline></fo:block></fo:block-container>
